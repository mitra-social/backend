{
  "name": "mitra-social/backend",
  "license": "MIT",
  "repositories": [
    {"type": "vcs", "no-api": true, "url": "https://github.com/mitra-social/php-env.git"},
    {"type": "vcs", "no-api": true, "url": "https://github.com/mitra-social/activitypub.git"},
    {"type": "vcs", "no-api": true, "url": "https://github.com/mitra-social/activity-streams.git"}
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "cache/array-adapter": "^1.0",
    "chubbyphp/chubbyphp-config": "^2.1",
    "chubbyphp/chubbyphp-doctrine-db-service-provider": "^1.6.1",
    "doctrine/dbal": "^2.10.1",
    "doctrine/migrations": "^2.1.1",
    "doctrine/orm": "^2.7.1",
    "landrok/activitypub": "dev-improvements-destatized@dev",
    "league/tactician": "2.0-rc1",
    "mitra-social/php-env": "dev-master@dev",
    "monolog/monolog": "^2.0.2",
    "pimple/pimple": "^3.2.3",
    "ramsey/uuid": "^3.9.2",
    "react/event-loop": "^1.1",
    "react/http": "^0.8.5",
    "slim/http": "^1.0",
    "slim/psr7": "^1.0",
    "slim/slim": "^4.4.0",
    "symfony/validator": "^5.0.4",
    "tuupola/slim-jwt-auth": "^3.4"
  },
  "require-dev": {
    "ergebnis/phpstan-rules": "^0.14.3",
    "phpspec/phpspec": "^6.1.1",
    "phpstan/phpstan": "^0.12.11",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpunit/phpunit": "^8.5.2",
    "squizlabs/php_codesniffer": "^3.5.4",
    "friends-of-phpspec/phpspec-code-coverage": "^4.3",
    "cache/void-adapter": "^1.0",
    "rregeer/phpunit-coverage-check": "^0.3.1",
    "doctrine/data-fixtures": "^1.4.2"
  },
  "autoload": {
    "psr-4": {
      "Mitra\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mitra\\Fixtures\\": "fixtures/"
    }
  }
}
